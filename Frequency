arr=list(map(int,input("enter number seperated by space:").split()))
freq={}
for num in arr:
    freq[num]=freq.get(num,0)+1
print("frequency of each element:")
for key ,value in freq.items():
    print(f"{key}:{value}")
max_freq=max(freq.values())
most_frequent=[key for key,value in freq.items() if value==max_freq]
print("\nelement(s) with highest frequency:",most_frequent)
print("highest frequency count:",max_freq)
